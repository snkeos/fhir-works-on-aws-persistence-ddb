{
  "name": "fhir-works-on-aws-persistence-ddb",
  "version": "3.11.0",
  "description": "FHIR Works on AWS persistence DynamoDB implementation",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": {
    "name": "Amazon Web Services",
    "email": "fhir-works-on-aws-dev@amazon.com",
    "url": "https://aws.amazon.com/"
  },
  "contributors": [
    "Robert Smayda <smayda@amazon.com>",
    "Tim Nguyen <thingut@amazon.com>",
    "Nestor Carvantes <nestorba@amazon.com>"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx",
    "lint-fix": "eslint --fix . --ext .ts,.tsx",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest --silent --ci --testResultsProcessor=jest-junit",
    "test-coverage": "jest --coverage",
    "release": "yarn run build && yarn run lint && yarn run test",
    "clean": "rm -rf build/* node_modules/* dist/* .serverless/* .nyc_output/* lib/*",
    "local": "node .",
    "prepublish": "tsc",
    "standard-version": "standard-version --skip.tag=true"
  },
  "dependencies": {
    "@elastic/elasticsearch": "7.13.0",
    "@types/aws-lambda": "8.10.83",
    "aws-elasticsearch-connector": "8.3.0",
    "aws-sdk": "2.1000.0",
    "aws-xray-sdk": "3.3.3",
    "fhir-works-on-aws-interface": "link:../fhir-works-on-aws-interface",
    "flat": "5.0.2",
    "lodash": "4.17.21",
    "mime-types": "2.1.33",
    "promise.allsettled": "1.0.5",
    "uuid": "3.4.0"
  },
  "devDependencies": {
    "@elastic/elasticsearch-mock": "0.3.1",
    "@types/flat": "5.0.2",
    "@types/jest": "26.0.24",
    "@types/lodash": "4.14.175",
    "@types/mime-types": "2.1.1",
    "@types/node": "18.19.10",
    "@types/promise.allsettled": "1.0.3",
    "@types/sinon": "9.0.11",
    "@types/uuid": "3.4.10",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "aws-sdk-mock": "5.4.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "26.6.3",
    "jest-each": "26.6.2",
    "jest-junit": "14.0.1",
    "jest-mock-extended": "1.0.18",
    "prettier": "2.4.1",
    "sinon": "9.2.4",
    "standard-version": "9.3.2",
    "ts-jest": "26.5.6",
    "typescript": "4.4.3"
  },
  "resolutions": {
    "path-parse": "^1.0.7",
    "set-value": "^4.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true,
    "transform": {
      "\\.(ts)$": "ts-jest"
    },
    "testRegex": ".test.ts"
  },
  "jest-junit": {
    "suiteName": "jest test reports",
    "outputDirectory": "./../test-reports",
    "outputName": "fhir-works-on-aws-persistence-ddb-reports.xml"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/awslabs/fhir-works-on-aws-persistence-ddb.git"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
